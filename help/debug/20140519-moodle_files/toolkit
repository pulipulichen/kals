function Dialog_close_link(_lang, _callback, _arg) {
    
    Dialog_close_option.call(this, _lang, _callback, _arg);
    
    //$.test_msg('Dialog_close_link()', [this._$option_prototype, Dialog_link.prototype._$option_prototype]);
}

